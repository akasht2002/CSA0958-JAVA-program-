
import java.util.*;
public class reversestring{
    public static void main(String[] args){
        int reverse=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("Number=");
        int n= sc.nextInt();
        while(n!=0){
            int remainder=n%10;
            reverse=reverse*10+remainder;
            n=n/10;
        }
        System.out.println("The reversed number is:"+reverse);
    }
}
